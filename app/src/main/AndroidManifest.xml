<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="sen.calfit.mobileapps.comsen.cat.com">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher2_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-6166270378015046~4237736401"/>

        <activity android:name="sen.calfit.mobileapps.comsen.cat.com.activities.ReceptaBBDD"></activity>
        <activity android:name="sen.calfit.mobileapps.comsen.cat.com.activities.Rfit"></activity>
        <activity android:name="sen.calfit.mobileapps.comsen.cat.com.activities.Rvegan"></activity>
        <activity android:name="sen.calfit.mobileapps.comsen.cat.com.activities.Rpostres"></activity>


        <activity android:name=".activities.MainActivity"
        android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="Instantiatable"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="sen.calfit.mobileapps.comsen.cat.com.activities.ReceptaActivity"
            android:parentActivityName=".activities.Rfit"/>

        <activity android:name="sen.calfit.mobileapps.comsen.cat.com.activities.ReceptaActivity2"
            android:parentActivityName=".activities.Rvegan"/>
        <activity android:name="sen.calfit.mobileapps.comsen.cat.com.activities.ReceptaActivity3"
            android:parentActivityName=".activities.Rpostres"/>
        <activity android:name="sen.calfit.mobileapps.comsen.cat.com.activities.VideoPlayerActivity"></activity>
    </application>

</manifest>